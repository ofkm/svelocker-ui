{
  "name": "svelocker-ui",
  "private": true,
  "version": "0.24.0",
  "type": "module",
  "scripts": {
    "dev": "vite dev --host",
    "build": "vite build",
    "preview": "vite preview --port 3000",
    "prepare": "svelte-kit sync || echo ''",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint . && prettier --check .",
    "format": "prettier --write .",
    "test:e2e": "PLAYWRIGHT=true DB_PATH=test.db PUBLIC_REGISTRY_URL=http://localhost:5001 playwright test",
    "test:e2e:ui": "PLAYWRIGHT=true DB_PATH=test.db playwright test --ui",
    "test:e2e:debug": "PLAYWRIGHT=true DB_PATH=test.db playwright test --debug"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.7",
    "@eslint/js": "^9.23.0",
    "@playwright/test": "^1.51.1",
    "@sveltejs/adapter-auto": "^5.0.0",
    "@sveltejs/kit": "^2.20.2",
    "@sveltejs/vite-plugin-svelte": "^5.0.0",
    "@types/node": "^22.13.14",
    "bits-ui": "^1.3.14",
    "clsx": "^2.1.1",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-svelte": "^3.3.3",
    "globals": "^16.0.0",
    "lucide-svelte": "^0.485.0",
    "mode-watcher": "^0.5.1",
    "prettier": "^3.5.3",
    "prettier-plugin-svelte": "^3.3.3",
    "svelte": "^5.25.3",
    "svelte-check": "^4.1.5",
    "svelte-sonner": "^0.3.28",
    "tailwind-merge": "^3.0.2",
    "tailwind-variants": "^1.0.0",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.28.0",
    "vaul-svelte": "^1.0.0-next.7",
    "vite": "^6.2.3"
  },
  "dependencies": {
    "@lucide/svelte": "^0.485.0",
    "@sveltejs/adapter-node": "^5.2.12",
    "@tailwindcss/vite": "^4.0.17",
    "@tanstack/table-core": "^8.21.2",
    "@types/axios": "^0.14.4",
    "@types/better-sqlite3": "^7.6.12",
    "@types/node-cron": "^3.0.11",
    "axios": "^1.8.4",
    "better-sqlite3": "^11.9.1",
    "buffer": "^6.0.3",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.7",
    "json": "^11.0.0",
    "node-cron": "^3.0.3",
    "stream-browserify": "^3.0.0",
    "tailwindcss": "^4.0.12"
  }
}
